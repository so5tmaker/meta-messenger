{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Account": {
      "type": "object",
      "properties": {
        "accountId": {
          "type": "integer"
        },
        "login": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "AccountRole": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AccountRole"
          }
        },
        "Area": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Area"
          }
        },
        "AreaAccount": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AreaAccount"
          }
        },
        "Message": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Message"
          }
        },
        "Session": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        }
      },
      "required": [
        "login",
        "password"
      ]
    },
    "AccountRole": {
      "type": "object",
      "properties": {
        "Account": {
          "$ref": "#/definitions/Account"
        },
        "Role": {
          "$ref": "#/definitions/Role"
        }
      },
      "required": [
        "accountId",
        "roleId"
      ]
    },
    "Area": {
      "type": "object",
      "properties": {
        "areaId": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "Account": {
          "$ref": "#/definitions/Account"
        },
        "AreaAccount": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AreaAccount"
          }
        },
        "Message": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Message"
          }
        }
      },
      "required": [
        "name",
        "ownerId"
      ]
    },
    "AreaAccount": {
      "type": "object",
      "properties": {
        "Account": {
          "$ref": "#/definitions/Account"
        },
        "Area": {
          "$ref": "#/definitions/Area"
        }
      },
      "required": [
        "areaId",
        "accountId"
      ]
    },
    "Message": {
      "type": "object",
      "properties": {
        "messageId": {
          "type": "integer"
        },
        "text": {
          "type": "string"
        },
        "Area": {
          "$ref": "#/definitions/Area"
        },
        "Account": {
          "$ref": "#/definitions/Account"
        }
      },
      "required": [
        "areaId",
        "fromId",
        "text"
      ]
    },
    "Role": {
      "type": "object",
      "properties": {
        "roleId": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "AccountRole": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AccountRole"
          }
        }
      },
      "required": [
        "name"
      ]
    },
    "Session": {
      "type": "object",
      "properties": {
        "sessionId": {
          "type": "integer"
        },
        "token": {
          "type": "string"
        },
        "ip": {
          "type": "string"
        },
        "data": {
          "type": [
            "number",
            "string",
            "boolean",
            "object",
            "array",
            "null"
          ]
        },
        "Account": {
          "$ref": "#/definitions/Account"
        }
      },
      "required": [
        "accountId",
        "token",
        "ip",
        "data"
      ]
    }
  },
  "type": "object",
  "properties": {
    "account": {
      "$ref": "#/definitions/Account"
    },
    "accountRole": {
      "$ref": "#/definitions/AccountRole"
    },
    "area": {
      "$ref": "#/definitions/Area"
    },
    "areaAccount": {
      "$ref": "#/definitions/AreaAccount"
    },
    "message": {
      "$ref": "#/definitions/Message"
    },
    "role": {
      "$ref": "#/definitions/Role"
    },
    "session": {
      "$ref": "#/definitions/Session"
    }
  }
}